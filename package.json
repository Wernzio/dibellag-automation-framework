{
  "name": "dibellag-automation-framework",
  "version": "1.0.0",
  "description": "Protractor Cucumber framework using latest package versions",
  "main": "protractor.conf.js",
  "scripts": {
    "pretest": "webdriver-manager update && npm run lint",
    "test": "npm run report:cleanup && protractor protractor.conf.js",
    "test:ci": "protractor protractor.conf.js",
    "test:local": "(npm test; npm run report:generate)",
    "test:shard": "protractor protractor-sharded.conf.js",
    "test:elementExplorer": "protractor protractor.conf.js --seleniumAddress=http://127.0.0.1:4444/wd/hub --elementExplorer",
    "test:dry-run": "protractor protractor.conf.js --cucumberOpts.dry-run",
    "test:scripts:parallel": "./test/scripts/parallel.sh",
    "test:scripts:synchronous": "./test/scripts/synchronous.sh",
    "test:scripts:ignore-failure": "./test/scripts/ignore-failure.sh",
    "report:cleanup": "rm -rf test/output && mkdir test/output",
    "report:generate": "node ./test/lib/reporter.config.js",
    "lint": "(npm run lint:gherkin; npm run lint:es)",
    "webdriver-manager:update": "webdriver-manager update",
    "webdriver-manager:start": "webdriver-manager start",
    "webdriver-manager:setup": "webdriver-manager update && webdriver-manager start",
    "lint:fix": "eslint test/** --fix",
    "lint:gherkin": "gherkin-lint",
    "lint:es": "eslint test/**"
  },
  "author": "Giuseppe DiBella",
  "license": "ISC",
  "dependencies": {
    "babel-eslint": "^8.0.0",
    "bluebird": "^3.5.0",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "chance": "^1.0.10",
    "cucumber": "^3.0.2",
    "cucumber-html-reporter": "^3.0.1",
    "eslint": "^4.7.0",
    "eslint-plugin-protractor": "^1.40.0",
    "gherkin-lint": "^2.5.1",
    "glob": "^7.1.2",
    "lodash": "^4.17.4",
    "page-object-loader": "^1.0.1",
    "protractor": "^5.1.2",
    "protractor-cucumber-framework": "^4.0.8",
    "yargs": "^9.0.1"
  }
}
